{
  "name": "code-whisperer",
  "version": "0.7.0",
  "description": "Twitch Code Whisperer for Party Game Streamers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcpesses/code-whisperer.git"
  },
  "keywords": [
    "react",
    "redux",
    "vite",
    "typescript",
    "template",
    "ghactions",
    "vitest",
    "twitch",
    "oauth"
  ],
  "author": "Daniel Pesses",
  "license": "MIT",
  "homepage": "https://github.com/dcpesses/code-whisperer",
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:ci": "tsc && vite build && cp 404.html dist/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "serve": "vite preview",
    "prepare": "husky && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "lint": "eslint --cache --fix ."
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/prompt-cli": "^19.7.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/node": "^18.19.20",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "happy-dom": "^13.10.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.4.4",
    "vite": "^4.5.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^0.34.1"
  }
}
